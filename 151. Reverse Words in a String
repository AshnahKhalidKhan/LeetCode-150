class Solution 
{
    public String reverseWords(String s) 
    {
        String outputString = "";
        String currentWord = "";
        int stringLength = s.length();
        boolean newWordStarted = false;
        for (int i = 0; i < stringLength; i++) 
        {
            System.out.println("." + s.charAt(i) + ".");
            if (s.charAt(i) == ' ') 
            {
                // if (newWordStarted == true)
                // {
                // outputString = currentWord + outputString;
                // currentWord = "";
                // newWordStarted = false;
                // }
                // else
                // {
                newWordStarted = false;
                continue;
                // }
            }
            else if (s.charAt(i) != ' ') 
            {
                if (newWordStarted == false) 
                {
                    outputString = " " + currentWord + outputString;
                    currentWord = "";
                }
                newWordStarted = true;
                currentWord = currentWord + s.charAt(i);
                if (i == stringLength - 1)
                {
                    outputString = currentWord + outputString;
                    currentWord = "";
                }
            }
            System.out.println("currentWord: ." + currentWord + ". Outputstring: ." + outputString + ".");
        }
        return outputString;
    }
}
