class Solution 
{
    public boolean increasingTriplet(int[] nums) 
    {
        int i = 0;
        int j = 1;
        int k = 2;
        int numsArrayLength = nums.length;
        if (numsArrayLength < 3)
        {
            return false;
        }
        /*
        What if I hash this?
        nums = [2,1,5,0,4,6]
        0 = 3
        1 = 1
        2 = 0
        4 = 5
        5 = 2
        6 = 6
        Oh wait, I can't. O(1) space complexity needed.
        */
        return true;
    }
}
