class Solution 
{
    public void moveZeroes(int[] nums) 
    {
        // 0, 0, 1, 0, 3, 12, 0
        // 1, 0, 0, 0, 3, 12, 0
        // 1, 3, 12, 0, 0, 0, 0
        int numsArrayLength = nums.length;
        int indexOfLastZero = 0;
        for (int i = 0; i < numsArrayLength; i++)
        {
            if (nums[i] == 0)
            {
                while (i < numsArrayLength && nums[i] != 0)
                {
                    i++;
                }
                if (nums[i] != 0)
                {
                    nums[indexOfLastZero] = nums[i];
                    nums[i] = 0;
                    indexOfLastZero++;
                }
            }
            // if (nums[i] != 0 && indexOfLastZero > -1)
            // {
            //     nums[indexOfLastZero] = nums[i];
            //     nums[i] = 0;
            //     indexOfLastZero = i;
            // }
            // else //if (nums[i] == 0) or (indexOfLastZero == -1)
            // {
            //     indexOfLastZero = i;
            // }
        }
    }
}
